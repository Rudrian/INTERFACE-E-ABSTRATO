package Interface;

public class LeaoAbs extends AnimalAbs {
	
	 String nome;
     String sexo;
     String raca;

    @Override
    public void emitirSom() {
        System.out.println(nome + " est√° rugindo.");
    }

    public String getNome() {
        return nome;
    }

    public void setNome(String nome) {
        this.nome = nome;
    }

    public String getSexo() {
        return sexo;
    }

    public void setSexo(String sexo) {
        this.sexo = sexo;
    }

    public String getRaca() {
        return raca;
    }

    public void setRaca(String raca) {
        this.raca = raca;
    }
}


